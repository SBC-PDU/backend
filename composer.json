	"require-dev": {
		"contributte/qa": "^v0.4.0",
		"contributte/tester": "^0.3.0",
		"iqrf/coding-style": "^0.1.1",
		"iqrf/phpstan-config": "^0.1.2",
		"mockery/mockery": "^1.6.12",
		"rector/rector": "^2.0.3"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app",
			"Database\\": "db"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests"
		}
	},
	"support": {
		"email": "mail@romanondracek.cz",
		"issues": "https://github.com/SBC-PDU/backend/issues",
		"source": "https://github.com/SBC-PDU/backend"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"platform": {
			"php": "8.3.0"
		},
		"sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"composer/package-versions-deprecated": true,
			"php-http/discovery": true
		}
	}
}
